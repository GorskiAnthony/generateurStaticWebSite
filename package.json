{
	"name": "generateurSiteStatic",
	"version": "1.0.0",
	"description": "Generate static website",
	"main": "index.js",
	"author": "Anthony Gorski",
	"scripts": {
		"dev:prebuild": "nodemon index.js",
		"dev": "cd dist && browser-sync start --server --watch",
		"start": "concurrently \"yarn run dev:prebuild\" \"yarn run dev\"",
		"build": "node index.js && parcel build ./dist/* ./dist/**/* --no-minify"
	},
	"license": "MIT",
	"dependencies": {
		"parcel-bundler": "^1.12.4"
	},
	"devDependencies": {
		"concurrently": "^5.3.0",
		"browser-sync": "^2.26.14",
		"nodemon": "^2.0.7"
	}
}
